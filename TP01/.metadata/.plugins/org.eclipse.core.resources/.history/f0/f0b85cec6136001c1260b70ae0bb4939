
public class ex03{

	public static void main (String[] args){
		// A. 
		int i, j;
		
	    for (i = 1; i <= 6; i++)
	    {
	        for (j = 1; j <= 6; j++)
	        {
	            if (i == 1 || i == 6 ||
	                j == 1 || j == 6)        
	                System.out.println("*");           
	            else
	            	System.out.println(" ");  
	        }
	        System.out.println("\n");  
	    }
        
        // B. 
        
        
	}
}